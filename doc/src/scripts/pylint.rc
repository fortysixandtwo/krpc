[BASIC]
good-names=i,j,k,ex,Run,_,u,v,q,r,g,x,y,z,w,n,F,Isp,d
variable-rgx=[a-z_][a-z0-9_]{1,30}$
method-rgx=[a-z_][a-z0-9_]{2,60}$
const-rgx=[a-z_][a-z0-9_]{1,30}$

[DESIGN]
max-args=10
max-locals=20
max-attributes=10
max-returns=25
max-branches=25

[MESSAGES CONTROL]
disable=no-member,missing-docstring,similarities,superfluous-parens,import-error

[REPORTS]
reports=no
